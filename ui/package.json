{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "cra-template": "1.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "http-proxy-middleware": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --noStackTrace",
    "eject": "react-scripts eject",
    "test:ci": "yarn test --passWithNoTests --silent --forceExit --detectOpenHandles --watchAll=false --ci=true",
    "dist": "yarn test:ci && yarn build",
    "start:dev": "(yarn test &); yarn start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
